<h2>Restaurant: {{restaurant.name}}</h2>
<h4>Cusine: {{restaurant.cuisine}}</h4>
<h4>Location: {{restaurant.location}}</h4>
<img class="item-img" src="{{restaurant.image}}" />
  {{#each restaurant.Menus}}
  <div class="row justify-content-center">
        <h5>{{this.title}} Menu</h5>
        <ul class="list-group list-group-flush">
        {{#each this.Items}}
            <li>{{this.name}} : ${{this.price}}</li>
        {{/each}}
        </ul>
    </div>
  {{/each}}
<div>
    <a href="/restaurants">Back to Restaurant List</a>
</div>